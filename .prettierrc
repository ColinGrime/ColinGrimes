{
    "plugins": ["prettier-plugin-tailwindcss"],
    "printWidth": 160,
    "tabWidth": 4,
    "semi": true,
    "useTabs": false,
    "bracketSpacing": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "arrowParens": "always",
    "trailingComma": "es5",
    "proseWrap": "preserve",
    "endOfLine": "lf"
}
